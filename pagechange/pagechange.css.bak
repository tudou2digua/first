body,div,section,h2,h3,a,input{
margin:0;
padding:0;
list-style:none;
}
html,body{
margin:0;
height:100%;
overflow:hidden;
}
@font-face{
font-family:"Raphaelicons";
src:url('fonts/raphaelicons-webfont.eot') format('eot'),
	url('fonts/raphaelicons-webfont.svg') format('svg'),
	url('fonts/raphaelicons-webfont.ttf') format('ttf'),
	url('fonts/raphaelicons-webfont.woff') format('woff');
	font-weight:normal;
	font-style:normal;
	}

.container{
width:100%;
height:100%;
position:relative;
}
.st-container{
	width:100%;
	height:100%;
	position:absolute;
	}
.toalpage{
width:100%;
height:100%;
-webkit-transition:-webkit-transform .5s;
text-align:center;
}
.st-container a,.st-container input{
display:inline-block;
width:20%;
text-align:center;
cursor:pointer;
position:fixed;
bottom:0;
height:34px;
line-height:34px;
}
 .st-container a{
z-index:9;
color:#fff;
font-size:34px;
font-weight:900;
background-color:#ff3e96;
text-decoration:none;
box-sizing:border-box;
border:1px solid #ffffff;
}
.st-container input{
z-index:10;
opacity:0;
}
#page1,#page1 + a{
left:0%;
}
#page2,#page2 + a{
left:20%;
}
#page3,#page3 + a{
left:40%;
}
#page4,#page4 + a{
left:60%;
}
#page5,#page5 + a{
left:80%;
}
.st-container input:hover + a{
background-color:#cd2626;
}
.st-container input:checked + a{
background-color:#a52a2a;
}
.st-container input:checked + a:after{
content:'';
width:0;
height:0;
border:20px solid transparent;
border-bottom:30px solid #a52a2a;
position:absolute;
left:50%;
margin-left:-20px;
top:-45px;
}
.page{
background-color:#eea9b8;
width:100%;
height:100%;
text-align:center;
box-sizing:border-box;
position:relative;
overflow:hidden;
}
.page h2 {
font-size:40px;
color:#a0522d;
}
.page p{
font-size:16px;
color:#a0522d;
}
 #page1:checked ~ .toalpage {
-webkit-transform:translate3d(0,0,0);
}
 #page2:checked ~ .toalpage{
-webkit-transform:translate3d(0,-100%,0);
}
#page3:checked ~ .toalpage{
-webkit-transform:translate3d(0,-200%,0);
}
 #page4:checked ~ .toalpage{
-webkit-transform:translate3d(0,-300%,0);
}
 #page5:checked ~ .toalpage{
-webkit-transform:translate3d(0,-400%,0);
}
.box{
width:300px;
height:300px;
background-color:#eea9b8;
margin:0px auto;
top:0;
color:#fff;
-webkit-transform: translate3d(0,-150px,0) rotate(45deg);
z-index:20;
position:relative;
}
.colorChang{
background-color:#ffffff;
color:#eea9b8;
}
[data-icon]:after{
content:attr(data-icon);
height:200px;
width:200px;
font-size:200px;
position:absolute;
top:50%;
left:50%;
-webkit-transform:rotate(-45deg) translateY(25%);
font-family:"Raphaelicons";
}